<!-- #justin is working on profile -->
<p><%= @user.username %></p>

<h2>Surveys Created</h2>
<% @surveys.each do |survey| %>
  <h3><%= survey.name %></h3>
  <a href="/survey/results/<%=survey.id%>">View</a>
  <a href="/delete/survey/<%=survey.id%>">Delete</a>
<% end %>

<h2>Surveys Answered</h2>
<% @answered_surveys.each do |survey| %>
  <h3><%= survey.name %></h3>
  <a href="/survey/results/<%=survey.id%>">View</a>
<% end %>
