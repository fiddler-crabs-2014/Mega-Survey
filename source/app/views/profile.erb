

<div class="container" id="profile_list">
<h2>Welcome <%= @user.username %></h2>

<h3>Surveys Created:</h3>
<ul>
<% @surveys.each do |survey| %>
  <li><%= survey.name %>
    <br>
  <a href="/survey/results/<%=survey.id%>">View</a> &nbsp;&nbsp;-&nbsp;&nbsp;
  <a href="/delete/survey/<%=survey.id%>">Delete</a>
  <br>
  <br>
<% end %>
</ul>

<h3>Surveys Answered:</h3>
<ul>
<% @answered_surveys.each do |survey| %>
  <li><%= survey.name %><br>
  <a href="/survey/results/<%=survey.id%>">View</a>
  <br>
  <br>
<% end %>
</ul>
</div>
